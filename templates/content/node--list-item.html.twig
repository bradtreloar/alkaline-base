{#
/**
 * @file
 * Theme override to display a node.
 */
#}
{{ attach_library('alkaline/media_object') }}
{%
  set classes = [
    'media-object',
  ]
%}
<article{{ attributes.addClass(classes) }}>

  {{ content.field_image }}

  <header>
    {{ title_prefix }}
    <h2{{ title_attributes }}>
      <a href="{{ url }}" rel="bookmark">{{ label }}</a>
    </h2>
    {{ title_suffix }}
  </header>

</article>
