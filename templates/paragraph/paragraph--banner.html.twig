{% extends "paragraph.html.twig" %}
{#
/**
 * @file
 * Custom theme implementation to display a paragraph.
 */
#}
{% block content %}
  {{ attach_library('alkaline/banner') }}
  <div {{ attribute.addClass("row") }}>
    {# <div> #}
      <figure class="grid small-1 medium-2">
        {{ content.field_image }}
        <figcaption>
          {{ content|without('field_image') }}
        </figcaption>
      </figure>
    {# </div> #}
  </div>
  {{ css }}
{% endblock %}

