{% extends "paragraph.html.twig" %}
{#
/**
 * @file
 * Theme override for a paragraph type.
#}
{% block content %}
  {{ attach_library('alkaline/card') }}
  <figure class="card">
    {{ content.field_image }}
    <figcaption>
      {{ content|without('field_image') }}
    </figcaption>
  </figure>
{% endblock %}
