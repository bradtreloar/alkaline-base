{% extends "paragraph--body-section.html.twig" %}
{#
/**
 * @file
 * Theme override for a paragraph type.
#}
{% block content %}
  {{ attach_library('alkaline/media_object') }}
  <div{{ attributes.addClass("media-object") }}>
    {% if content.field_image %}
      <figure>
        {{ content.field_image }}
      </figure>
    {% endif %}
    <div>
      {{ content|without('field_image') }}
    </div>
  </div>
{% endblock %}
