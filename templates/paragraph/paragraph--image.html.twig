{% extends "paragraph.html.twig" %}
{#
/**
 * @file
 * Theme override for a paragraph type.
#}
{% block content %}
  <div class="row">
    {# <div> #}
      <figure class="image">
        {{ content.field_image }}
        {% if content.field_caption %}
          <figcaption>
            {{ content.field_caption }}
          </figcaption>
        {% endif %}
      </figure>
    {# </div> #}
  </div>
{% endblock %}
