{% extends "paragraph.html.twig" %}
{#
/**
 * @file
 * Theme override for a paragraph type.
#}
{% block content %}
{{ attach_library('alkaline/card') }}
{%
  set classes = [
    'card',
  ]
%}
<figure {{ attributes.addClass(classes) }}>
  {{ content.field_image }}
  <figcaption>
    {{ content|without('field_image') }}
  </figcaption>
</figure>
{% endblock %}
