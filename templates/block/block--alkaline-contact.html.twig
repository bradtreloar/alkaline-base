{#
/**
 * @file
 * Theme override to display a block.
 */
#}
{%
  set classes = [
    'block',
    'block-' ~ configuration.provider|clean_class,
    'block-' ~ plugin_id|clean_class,
    'block--contact',
  ]
%}
<div{{ attributes.addClass(classes) }}>
  {{ title_prefix }}
  {% if label %}
    <h2{{ title_attributes }}>{{ label }}</h2>
  {% endif %}
  {{ title_suffix }}
  {% block content %}
    {% if content %}
      {% if content.field_map %}
      <div>
        <section>
          {{ content|without('field_map') }}
        </section>
        <section>
          {{ content.field_map }}
        </section>
      </div>
      {% else %}
      <section>
        {{ content }}
      </section>
      {% endif %}
    {% endif %}
  {% endblock %}
</div>
